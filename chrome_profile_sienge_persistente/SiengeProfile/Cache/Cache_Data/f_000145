<script type="text/javascript">
	function login() {
		jQuery('#divEsqueceuSenha').hide();
		jQuery('#divLogin').show();
	}

	function fazerLoginComSiengeId() {
		toggleBotaoLogin({disabled: true});
		window.location = "/sienge/internal-api/v1/auth/sso/login?did="+jQuery('#did_hidden').val();
	}

	function novoUsuario() {
		jQuery('#divEsqueceuSenha').hide();
		jQuery('#divLogin').hide();
	}

	function esqueceuSenha() {
		jQuery('#divEsqueceuSenha').show();
		jQuery('#divLogin').hide();
	}

	function enviaEmailPassword() {
		var cdUsuario = jQuery('#divEsqueceuSenha input[id=username]').val();
		if (cdUsuario != null && cdUsuario.trim() !== '') {
			document.getElementById("btnEnviarEmail").disabled = true;
			window.location = '/sienge/esqueciMinhaSenha.do?cdUsuario=' + cdUsuario;
			
		} else {
			alert(resourceMap.getResource('label.js.identificacaoObrigatorio'));
		}
		esqueceuSenha();
	}

	function pegarUsernameCorreto(formId) {
		var campoUsernameHidden = jQuery("#"+formId+" input[name='username']");
		var campoUsername = jQuery("#"+formId+" input[id='username']");
		campoUsernameHidden.attr('value',campoUsername.val().toLowerCase());
		var userName = getValue('username');
		if("" != userName){
			sessionStorage.setItem("username", userName);
		}
	}

	function adicionarEspacoNoUsername() {
		var campoUsername = jQuery('#username');
		campoUsername.attr('value',campoUsername.val() + " ");
	}


	function adicionarEspacoNoUsername() {
		var campoUsername = jQuery('#username');
		campoUsername.attr('value',campoUsername.val() + " ");
	}


	function onLoadLogin() {
		validaVersaoNavegador();
		checaJavaScript();
		onFocus();
		generateAndSetDID();
		clearMenuLocalStorage();
	}
	
	function clearMenuLocalStorage(){
		const menuCache = localStorage.getItem("sienge-sidebar-data");
        if(menuCache){
            const menuData = JSON.parse(menuCache);
            const newMenuData ={...menuData, open: false, selectedModule: {...menuData.selectedModule, data: null}};
            localStorage.setItem("sienge-sidebar-data", JSON.stringify(newMenuData));
        }
	}

	function validaVersaoNavegador() {
		if (isRunningIE7OrBelow) {
			document.getElementById('versaoNavegadorDesatualizado').style.display = 'block';
			document.getElementById('versaoNavegadorDesatualizadoTitulo').innerHTML = 'Seu Internet Explorer está desatualizado.'
		}
		if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { //test for Firefox/x.x or Firefox x.x (ignoring remaining digits);
			var ffversion=new Number(RegExp.$1) // capture x.x portion and store as a number
			if (ffversion < 3.6) {
				document.getElementById('versaoNavegadorDesatualizado').style.display = 'block';
				document.getElementById('versaoNavegadorDesatualizadoTitulo').innerHTML = 'Seu Mozilla Firefox está desatualizado.'
			}
		}
		if (/Chrome[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { //test for Chrome/x.x or Chrome x.x (ignoring remaining digits);
			var cversion=new Number(RegExp.$1) // capture x.x portion and store as a number
			if (cversion < 12) {
				document.getElementById('versaoNavegadorDesatualizado').style.display = 'block';
				document.getElementById('versaoNavegadorDesatualizadoTitulo').innerHTML = 'Seu Google Chrome está desatualizado.'
			}
		}
	}

	function carregaNaRaizDoBrowser() {
		// variavel relogin é utilizada para não recarregar a página como janela principal quando esta tela for utilizada para alterar a senha e relogar o usuário, ou seja, não tira...
		if (window != window.parent && !window.parent.relogin) {
			window.parent.location.href='/sienge/avisos/sessaoExpirada.html';
			this.close();
		}
	}

	function esqueciMinhaSenha() {
		var cdUsuario = document.getElementById('username').value;
		if (cdUsuario != null && cdUsuario != '') {
			window.location = '/sienge/esqueciMinhaSenha.do?cdUsuario=' + cdUsuario;
		} else {
			alert(resourceMap.getResource('label.js.identificacaoObrigatorio'));
		}
	}
	

	function onFocus() {
		if (true) {
			document.getElementById('submit').disabled = false;
			document.getElementById('username').focus();
		}
	}

	function generateAndSetDID() {
		let uuid = ('; '+document.cookie).split(`; MVNBF3W3pfUQa7XvMVyUFlph8Q=`).pop().split(';')[0];
		if (!uuid) {
			DID.get().then(hash => {
				document.cookie = "MVNBF3W3pfUQa7XvMVyUFlph8Q="+hash+"; expires= "+ new Date(new Date().setFullYear(new Date().getFullYear() + 10)).toUTCString()+"; path=/; domain=.sienge.com.br";
				document.getElementById("did_hidden").value = hash;
			});
		}
		document.getElementById("did_hidden").value = uuid;
	}

	function popup(url){
		var objWin = window.open(url, '_popup', 'status=yes, toolbar=no, scrollbars=yes, titlebar=no, menubar=no, resizable=no, width=640, height=320, directories=no, location=no');
		if(!objWin){
			alert("O seu browser possui bloqueador de pop-up.\nVocê precisa desativá-lo.");
		}
	}

	function checaJavaScript(){
		document.getElementById('checkJavaScript').style.display = 'none';
	}

	

	function forcaLoginPaginaPrincipal() {
		if (false) {
			var locationPadrao = 'https://pratiemp.sienge.com.br' + '/sienge/';
			if (top.location != locationPadrao) {
				top.location = locationPadrao;
			}
		}
	}
	
	function sessaoAtiva() {
		popupCentralizadoCustomizado('/sienge/sessaoAtiva',640,380);
	}
	
	
	if (true) {
		togglePasswordVisibility("password", "password-toggle", "show-password", "hide-password");
	}
	
	function toggleBotaoLogin({disabled}) {
		if (true) {
			document.getElementById("submit").disabled = disabled
		}
		if (true) {
			document.getElementById("btnEntrarComSiengeID").disabled = disabled
		}
	}

	(function () {
		// Habilita os botões de login caso a página seja recarregada, apertando o voltar do navegador, por exemplo. 
		window.onpageshow = function(event) {
			if (event.persisted) {
				toggleBotaoLogin({disabled: false});
			}
		};
		// Habilita os botões de login caso a pessoa aperte ESC
		document.addEventListener('keydown', function(event) {
			if (event.key === "Escape") {
				toggleBotaoLogin({disabled: false});
			}
		});
	})();
	
</script>
</body>
</html>
